{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PostsResponse",
  "description": "PostsResponse contains a list of posts",
  "type": "object",
  "required": [
    "posts"
  ],
  "properties": {
    "posts": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Post"
      }
    }
  },
  "definitions": {
    "Attachment": {
      "type": "object",
      "required": [
        "mime_type",
        "uri"
      ],
      "properties": {
        "mime_type": {
          "type": "string"
        },
        "tags": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "uri": {
          "type": "string"
        }
      }
    },
    "Attribute": {
      "description": "This file contains all the desmos related types used inside desmos' contracts",
      "type": "object",
      "required": [
        "key",
        "value"
      ],
      "properties": {
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "Poll": {
      "type": "object",
      "required": [
        "allows_answer_edits",
        "allows_multiple_answers",
        "end_date",
        "provided_answers",
        "question"
      ],
      "properties": {
        "allows_answer_edits": {
          "type": "boolean"
        },
        "allows_multiple_answers": {
          "type": "boolean"
        },
        "end_date": {
          "type": "string"
        },
        "provided_answers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ProvidedAnswer"
          }
        },
        "question": {
          "type": "string"
        }
      }
    },
    "Post": {
      "type": "object",
      "required": [
        "comments_state",
        "created",
        "creator",
        "last_edited",
        "message",
        "post_id",
        "subspace"
      ],
      "properties": {
        "additional_attributes": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Attribute"
          }
        },
        "attachments": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Attachment"
          }
        },
        "comments_state": {
          "type": "string"
        },
        "created": {
          "type": "string"
        },
        "creator": {
          "type": "string"
        },
        "last_edited": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "parent_id": {
          "type": [
            "string",
            "null"
          ]
        },
        "poll": {
          "anyOf": [
            {
              "$ref": "#/definitions/Poll"
            },
            {
              "type": "null"
            }
          ]
        },
        "post_id": {
          "type": "string"
        },
        "subspace": {
          "type": "string"
        }
      }
    },
    "ProvidedAnswer": {
      "type": "object",
      "required": [
        "answer_id",
        "text"
      ],
      "properties": {
        "answer_id": {
          "type": "string"
        },
        "text": {
          "type": "string"
        }
      }
    }
  }
}
