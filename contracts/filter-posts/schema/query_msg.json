{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "description": "GetFilteredPosts returns a list of filtered posts where each post has been reported at most (reports_limit - 1) time",
      "type": "object",
      "required": [
        "get_filtered_posts"
      ],
      "properties": {
        "get_filtered_posts": {
          "type": "object",
          "required": [
            "reports_limit"
          ],
          "properties": {
            "reports_limit": {
              "type": "integer",
              "format": "uint16",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "posts"
      ],
      "properties": {
        "posts": {
          "type": "object"
        }
      }
    }
  ]
}
